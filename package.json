{
  "name": "material-ui-component-lib-project-1",
  "version": "0.1.0",
  "description": "A React component wrapper library built on Material UI (MUI) v7 with custom theming, TypeScript support, and accessible components",
  "type": "module",
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/material-ui-component-lib-project-1.git"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./theme": {
      "types": "./dist/types/theme/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "build": "vite build && tsc --project tsconfig.build.json",
    "build:vite": "vite build",
    "build:types": "tsc --project tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src/",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@mui/material": "^7.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0"
  },
  "devDependencies": {
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "@vitejs/plugin-react": "^4.5.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@mui/types": "^7.0.0",
    "eslint": "^9.0.0",
    "@eslint/js": "^9.0.0",
    "typescript-eslint": "^8.0.0",
    "prettier": "^3.5.0",
    "eslint-config-prettier": "^10.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@mui/material": "^7.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0"
  },
  "keywords": [
    "react",
    "material-ui",
    "mui",
    "component-library",
    "typescript",
    "design-system",
    "theming"
  ]
}
